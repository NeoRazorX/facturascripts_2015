<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : fs_logs.xml
    Author     : Carlos Garcia Gomez
    Description:
        Estructura de la tabla fs_logs.
-->
<tabla>
   <columna>
      <nombre>id</nombre>
      <tipo>serial</tipo>
      <nulo>NO</nulo>
      <defecto>nextval('fs_logs_id_seq'::regclass)</defecto>
   </columna>
   <columna>
      <nombre>tipo</nombre>
      <tipo>character varying(50)</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>detalle</nombre>
      <tipo>text</tipo>
      <nulo>NO</nulo>
   </columna>
   <columna>
      <nombre>fecha</nombre>
      <tipo>timestamp</tipo>
      <nulo>NO</nulo>
      <defecto>CURRENT_TIMESTAMP</defecto>
   </columna>
   <columna>
      <nombre>usuario</nombre>
      <tipo>character varying(12)</tipo>
   </columna>
   <columna>
      <nombre>ip</nombre>
      <tipo>character varying(40)</tipo>
      <nulo>YES</nulo>
   </columna>
   <columna>
      <nombre>alerta</nombre>
      <tipo>boolean</tipo>
   </columna>
   <restriccion>
      <nombre>fs_logs_pkey</nombre>
      <consulta>PRIMARY KEY (id)</consulta>
   </restriccion>
</tabla>
